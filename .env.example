# Webacy API Configuration
# Get your API key from https://developers.webacy.co/
WEBACY_API_KEY=your_webacy_api_key_here

# EVMAuth Configuration
# Contract address for token verification
EVMAUTH_CONTRACT_ADDRESS=0x9f2B42FB651b75CC3db4ef9FEd913A22BA4629Cf

# Chain ID (1223954 = Radius testnet)
EVMAUTH_CHAIN_ID=1223954

# RPC URL for blockchain interactions
EVMAUTH_RPC_URL=https://rpc.radiustech.io

# JWT Configuration (optional)
EVMAUTH_JWT_SECRET=evmauth-alpha-7f8a9b2c3d4e5f6a
# EVMAUTH_JWT_ISSUER=
# EVMAUTH_EXPECTED_AUDIENCE=

# Cache Configuration (optional)
# EVMAUTH_CACHE_TTL=300
# EVMAUTH_CACHE_MAX_SIZE=1000
# EVMAUTH_CACHE_DISABLED=false

# Development Configuration
# Set to true to enable development mode (bypasses crypto validation)
EVMAUTH_DEV_MODE=false

# Debug mode for verbose logging
EVMAUTH_DEBUG=false

# Token Tier Configuration (optional - defaults shown)
# EVMAUTH_BASIC_TOKEN_ID=1
# EVMAUTH_PREMIUM_TOKEN_ID=3
# EVMAUTH_PRO_TOKEN_ID=5